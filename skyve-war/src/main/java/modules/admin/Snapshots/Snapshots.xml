<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<document name="Snapshots" 
			xmlns="http://www.skyve.org/xml/document"
			xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<singularAlias>Snapshots</singularAlias>
	<pluralAlias>Snapshots</pluralAlias>
	<iconStyleClass>fa-solid fa-camera</iconStyleClass>
	<attributes>
		<text name="moduleName">
			<displayName>admin.snapshots.moduleName.displayName</displayName>
			<domain>variant</domain>
			<length>100</length>
		</text>
		<text name="queryName">
			<displayName>admin.snapshots.queryName.displayName</displayName>
			<domain>dynamic</domain>
			<length>100</length>
		</text>
		<collection name="snapshotsToReorder" type="aggregation" ordered="true">
			<displayName>Snapshots</displayName>
			<documentName>Snapshot</documentName>
			<minCardinality>0</minCardinality>
