<?xml version="1.0" encoding="UTF-8"?>
<view xmlns="http://www.skyve.org/xml/view" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" name="_welcomeBanner"
	title="Dashboard" xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../schemas/view.xsd">
	<form border="true">
		<column responsiveWidth="2" />
		<column />
		<column responsiveWidth="2" />
		<column />
		<column responsiveWidth="2" />
		<column />
		<column />
		<column />
		<row>
			<item>
				<button action="SwitchToDesign" />
			</item>
			<item>
				<button action="SwitchToDisplay" />
			</item>
			<item>
				<textField binding="dashboardMenuName" />
			</item>
			<item>
				<textField binding="dashboardIconStyleClass">
					<onBlurHandlers>
						<rerender clientValidation="false" />
					</onBlurHandlers>
				</textField>
			</item>
			<item>
				<blurb escape="false" sanitise="none">
					<![CDATA[{dashboardIconMarkup}]]>
				</blurb>
			</item>
			<item>
				<combo binding="moduleName">
					<onChangedHandlers>
						<rerender clientValidation="false" />
					</onChangedHandlers>
				</combo>
			</item>
		</row>
	</form>
	<listMembership binding="roles" candidatesHeading="Module's roles" membersHeading="Roles that can access dashboard" />
</view>
