<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<router uxuiSelectorClassName="router.DefaultUxUiSelector"
			xmlns="http://www.skyve.org/xml/router"
			xsi:schemaLocation="http://www.skyve.org/xml/router ../schemas/router.xsd"
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<uxui name="desktop">
		<route outcome="/desktop/home.xhtml" />
	</uxui>
	<uxui name="startup">
		<route outcome="/external/startup.xhtml" />
	</uxui>
	<uxui name="external">
		<route outcome="/external/list.xhtml">
			<criteria webAction="l" />
		</route>
		<route outcome="/external/edit.xhtml">
			<criteria webAction="e" />
		</route>
		<route outcome="/external/map.xhtml">
			<criteria webAction="m" />
		</route>
		<route outcome="/external/home.xhtml" />
	</uxui>
	<uxui name="tablet">
		<route outcome="/external/list.xhtml">
			<criteria webAction="l" />
		</route>
		<route outcome="/external/edit.xhtml">
			<criteria webAction="e" />
		</route>
		<route outcome="/external/map.xhtml">
			<criteria webAction="m" />
		</route>
		<route outcome="/external/home.xhtml" />
	</uxui>
	<uxui name="phone">
		<route outcome="/external/list.xhtml">
			<criteria webAction="l" />
		</route>
		<route outcome="/external/edit.xhtml">
			<criteria webAction="e" />
		</route>
		<route outcome="/external/map.xhtml">
			<criteria webAction="m" />
		</route>
		<route outcome="/external/home.xhtml" />
	</uxui>
	<unsecured>/external/public.xhtml</unsecured>
</router>
