<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<view type="edit" 
		title="Office" 
		xsi:schemaLocation="http://www.skyve.org/xml/view ../../../../../schemas/view.xsd" 
		xmlns="http://www.skyve.org/xml/view" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <form pixelWidth="400" border="true" borderTitle="Office Details">
        <column pixelWidth="100"/>
        <column/>
        <row>
            <item>
                <default binding="levelUnit"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="buildingName"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="streetAddress"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="suburb"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="postCode"/>
            </item>
        </row>
        <row>
            <item>
                <default binding="phone"/>
            </item>
        </row>
    </form>
    <actions>
        <defaults/>
    </actions>
</view>

