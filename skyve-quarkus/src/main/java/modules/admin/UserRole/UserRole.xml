<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<document name="UserRole" 
			xmlns="http://www.skyve.org/xml/document" 
			xsi:schemaLocation="http://www.skyve.org/xml/document ../../../schemas/document.xsd" 
			xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<persistent name="ADM_SecurityUserRole" />
	<singularAlias>User Role</singularAlias>
	<pluralAlias>User Roles</pluralAlias>
	<parentDocument>User</parentDocument>
	<bizKey expression="{roleName}"/>
	<attributes>
		<text name="roleName" required="true">
			<displayName>admin.userRole.roleName.displayName</displayName>
			<domain>variant</domain>
			<length>60</length>
		</text>
	</attributes>
</document>
