			<displayName>Content Image</displayName>
		</content>
		<content name="contentSignature">
			<displayName>Content Signature</displayName>
		</content>
		<association name="lookupDescription" type="aggregation">
			<displayName>Lookup Description</displayName>
			<documentName>LookupDescription</documentName>
		</association>
		<collection name="containerGrid" type="child">
			<displayName>Container</displayName>
			<documentName>ContainerGrid</documentName>
			<minCardinality>0</minCardinality>
		</collection>
		<collection name="inlineGrid" type="child">
			<displayName>Inline</displayName>
			<documentName>InlineGrid</documentName>
			<minCardinality>0</minCardinality>
		</collection>
		<collection name="orderedGrid" type="child" ordered="true">
			<displayName>Ordered</displayName>
			<documentName>OrderedGrid</documentName>
			<minCardinality>0</minCardinality>
		</collection>
		<collection name="dataRepeater" type="child" ordered="true">
			<displayName>Data Repeater</displayName>
			<documentName>DataRepeater</documentName>
			<minCardinality>0</minCardinality>
		</collection>
